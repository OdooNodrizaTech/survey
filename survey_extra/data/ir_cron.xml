<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="cron_send_surveys_satisfaction_mail" forcecreate="True" model="ir.cron">
        <field name="name">Send Surveys Satisfaction Mail</field>
        <field name="active" eval="False"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_survey_survey"/>
        <field name="state">code</field>
        <field name="code">model.cron_send_surveys_satisfaction_mail()</field>
    </record>

